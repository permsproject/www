{
  "blocks": { "expect": "always", "error": true },
  "brackets": { "expect": "never", "error": true },
  "colons": { "expect": "never", "error": true },
  "colors": { "expect": "never", "error": true },
  "commaSpace": { "expect": "always", "error": true },
  "commentSpace": { "expect": "always", "error": true },
  "cssLiteral": { "expect": "never", "error": true },
  "depthLimit": { "expect": 7, "error": true },
  "duplicates": false,
  "efficient": { "expect": "always", "error": true },
  "exclude": [],
  "extendPref": false,
  "globalDupe": false,
  "groupOutputByFile": true,
  "indentPref": { "expect": 2, "error": true },
  "leadingZero": { "expect": "always", "error": true },
  "maxErrors": false,
  "maxWarnings": false,
  "mixed": { "expect": false, "error": true },
  "mixins": ["contain", "user-drag", "touch-action", "content-zooming"],
  "namingConvention": { "expect": "lowercase-dash", "error": true },
  "namingConventionStrict": false,
  "none": { "expect": "always", "error": true },
  "noImportant": true,
  "parenSpace": { "expect": "never", "error": true },
  "placeholders": "always",
  "prefixVarsWithDollar": { "expect": "always", "error": true },
  "quotePref": { "expect": "single", "error": true },
  "semicolons": { "expect": "never", "error": true },
  "sortOrder": false,
  "stackedProperties": { "expect": "never", "error": true },
  "trailingWhitespace": { "expect": "never", "error": true },
  "universal": { "expect": "never", "error": true },
  "valid": { "expect": true, "error": true },
  "zeroUnits": { "expect": "never", "error": true },
  "zIndexNormalize": { "expect": 5, "error": true }
}
